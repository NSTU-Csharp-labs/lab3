<Window    Background="#3c3c3c"    Icon="/Assets/logo.ico"    Title="Просмотр фотографий"    d:DesignHeight="450"    d:DesignWidth="800"    mc:Ignorable="d"    x:Class="lab3.Views.MainWindow"    xmlns="https://github.com/avaloniaui"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    xmlns:vm="using:lab3.ViewModels"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">    <Design.DataContext>        <vm:MainWindowViewModel />    </Design.DataContext>    <Grid>        <StackPanel            HorizontalAlignment="Center"            Margin="0,10,0,0"            Orientation="Horizontal"            Spacing="3"            VerticalAlignment="Top">            <Button CornerRadius="8">                <Image                    Height="{StaticResource IconSize}"                    Source="{SvgImage /Assets/draw_icon.svg}"                    Width="{StaticResource IconSize}" />            </Button>            <Button CornerRadius="{StaticResource ButtonCorner}">                <Image                    Height="{StaticResource IconSize}"                    Source="{SvgImage /Assets/effects_icon.svg}"                    Width="{StaticResource IconSize}" />            </Button>            <Button CornerRadius="{StaticResource ButtonCorner}">                <Image                    Height="{StaticResource IconSize}"                    Source="{SvgImage /Assets/rotate_icon.svg}"                    Width="{StaticResource IconSize}" />            </Button>        </StackPanel>        <Button            CornerRadius="{StaticResource ButtonCorner}"            HorizontalAlignment="Left"            Margin="40,0,0,0"            VerticalAlignment="Center">            <Image                Height="70"                Source="{SvgImage /Assets/arrow_back_icon.svg}"                Width="{StaticResource IconSize}" />        </Button>        <Button            CornerRadius="{StaticResource ButtonCorner}"            HorizontalAlignment="Right"            Margin="0,0,40,0"            VerticalAlignment="Center">            <Image                Height="70"                Source="{SvgImage /Assets/arrow_forward_icon.svg}"                Width="{StaticResource IconSize}" />        </Button>    </Grid></Window>